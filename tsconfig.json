
{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
	  "strict": true,
	  "baseUrl": ".",
	  "paths": {
		"@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"] ,
		"*": ["*", "src/*"]
	  },
	  "module": "esnext",
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "types": ["react", "react-native", "react-native-screens"],
	  "allowJs": true,
	  "noEmit": true,
	  "jsx": "react"
	},
	"include": ["src", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
  }
  